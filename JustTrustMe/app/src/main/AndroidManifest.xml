        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ButtonActivity"></activity>

        <meta-data
            android:name="com.kakao.sdk.Appkey"
            android:value="7b16bf536b619a3a2bcee88f81106c7d" />

        <activity android:name=".KakaoLogin"></activity>
        <activity android:name=".GoogleLoign"></activity>
        <activity android:name=".FindingWay">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BookMark" />
        <activity android:name=".RegisterContacts" />
        <activity android:name=".OtherSettings" />
        <activity android:name=".NoticeActivity" />
        <activity android:name=".FAQ" />
        <activity android:name=".Home" />
        <activity android:name=".Search" />
